package test.mypac;

public class Member {
	public int num;
	public String name;
	public String addr;
	
	// 기본 생성자
	public Member() {
		
	}
	
	// 값을 3개 전달받는 생성자(Constructor)
	public Member(int num, String name, String addr) {
		// 매개 변수에 전달되는 값을 필드에 저장하기
		this.num = num;
		this.name = name;
		this.addr = addr;
	}
}
