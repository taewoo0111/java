package test.main;

import test.mypac.Member;

public class MainClass09 {
	public static void main(String[] args) {
		// 기본 생성자
		Member m1 = new Member();
		Member m2 = new Member();
		Member m3 = new Member();
		
		m1.num = 1;
		m1.name = "김태우";
		m1.addr = "수내동";
		
		m2.num = 2;
		m2.name = "김구라";
		m2.addr = "노량진";
		
		m3.num = 3;
		m3.name = "해골";
		m3.addr = "행신동";
		
		// 매개변수 3개를 전달받는 생성자를 이용해서 객체 생성
		Member m4 = new Member(4, "정현우", "서현동");
		
	}
}
