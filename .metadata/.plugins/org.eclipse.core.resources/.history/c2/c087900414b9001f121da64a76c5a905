package test.main;

import test.mypac.Car;
import test.mypac.Drink;

public class MainClass03 {
	public static void main(String[] args) {
		/*
		 * Car 객체를 3개 만들어서
		 * 임의의 자동차이름과 가격을 필드에 가가 대입하는 코드를 작성해 보세요.
		 */
		Car c1 = new Car();
		c1.name = "소나타";
		c1.price = 2000;
		
		Car c2 = new Car();
		c2.name = "아반떼";
		c2.price = 1500;
		
		Car c3 = new Car();
		c3.name = "그렌져";
		c3.price = 3000;
		/*
		 * Drink 객체 이용
		 */
		Drink d1 = new Drink();
		d1.name = "아메리카노";
		d1.price = 2000;
		d1.size = "big";
		
		Drink d2 = new Drink();
		d2.name = "카페라떼";
		d2.price = 3000;
		d2.size = "small";
		
		System.out.println(c3); 
	}
}
